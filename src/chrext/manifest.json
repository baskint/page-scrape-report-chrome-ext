{
  "manifest_version": 2,
  "name": "Page Scrape Analysis Tool",
  "short_name": "Page Scrape Analysi Tool",
  "description": "This extension analyzes a website for page scraping and analysis",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "images/icon128.png"
  },
  "author": "Baskin Tapkan",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}
